{
  "id": "doom-emacs",
  "version": "0.0.4",
  "name": "Doom Emacs",
  "description": "Doom Emacs configuration",
  "options": {
    "version": {
      "default": "latest",
      "description": "Select the version to install.",
      "proposals": ["latest"],
      "type": "string"
    },
     "sync_config_from_git_web_url": {
      "default": "",
      "description": "Get a Doom Emacs config files from a git web url.",
      "type": "string"
    }
  },
  "dependsOn": {"ghcr.io/devcontainers-contrib/features/homebrew-package:1": {"package": "emacs"},
                "ghcr.io/devcontainers/features/github-cli:1": {}}
}
